{
    "name": "Human Computable Passwords",
    "description": "Password managment scheme using human computable functions",
    "version": "0.1",
    "manifest_version": 2,
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icons/pw-icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
    }],
    "icons": {"16": "icons/pw-icon.png", "128": "icons/pw-icon.png"},
    "permissions": [
        "storage",
        "tabs"
        ]
}
